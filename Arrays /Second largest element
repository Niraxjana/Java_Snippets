import java.util.Scanner;
class Solution {
    public int getSecondLargest(int[] arr) {
    int largest = 0;
    int second_big = 0;
    for (int i =0; i<arr.length;i++){
        if (arr[i] > largest){
            second_big = largest;
            largest = arr[i];
        }
        else if(arr[i]<largest && arr[i]>second_big){
            second_big = arr[i];
        }
    }
    return second_big;
    }
}
